all: $(NAME)

$(NAME): $(OBJS)
	@$(CC) $(OBJS) -o $@
	@make clean

%.o: %.c
	$(CC) $(CFLAGS) -c $< -o $@

clean:
	rm -rf $(OBJS)

fclean: clean
	rm -rf $(NAME)

a:
	@./push_swap $(ARG)

test1:
	@./push_swap $(ARG) | wc -l

test:
	@./push_swap $(ARG) | ./checker_Mac $(ARG)

pc:
	@bash push_swap_test.sh -v2 $(ARG)
.PHONY: all clean fclean test

